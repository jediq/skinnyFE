{
  "resources":[
    {
      "name":"Vehicle",
      "url":"http://localhost:9019/{{identifier}}"
    },
    {
      "name":"User",
      "url":"http://localhost:9020/{{identifier}}",
      "inputValidators":{
        "regexingParam":"(pass)"
      }
    }
  ],
  "name":"TEST INSTANCE",
  "defaultTemplates":"src/test/resources/basic/templates",
  "millisToCacheTemplates":0,
  "port":8008,
  "adminPort":8009,
  "assetsPath":"/assets",
  "assetsFolder":"src/test/resources/basic/assets/",

  "templates":[
    {
      "regex":".*\/pathed\/.*",
      "file":"src/test/resources/basic/templates/pathed.moustache",
      "enrichers":["src/test/resources/basic/enrichers/pathed.js"]
    },
    {
      "regex":".*\/enricherView",
      "file":"src/test/resources/basic/templates/enricherView.moustache",
      "enrichers":["src/test/resources/basic/enrichers/enricherView.js"]
    },
    {
      "regex":".*\/multipleEnricherView",
      "file":"src/test/resources/basic/templates/enricherView.moustache",
      "enrichers":["src/test/resources/basic/enrichers/enricherView.js",
                   "src/test/resources/basic/enrichers/multipleEnricherView.js"]
    },
    {
      "regex":".*\/enricherChangingResource",
      "file":"src/test/resources/basic/templates/enricherView.moustache",
      "enrichers":["src/test/resources/basic/enrichers/enricherChangingResource.js"]
    },
    {
      "regex":".*\/enricherChangingResourceBadly",
      "file":"src/test/resources/basic/templates/enricherView.moustache",
      "enrichers":["src/test/resources/basic/enrichers/enricherChangingResourceBadly.js"]
    }
  ],

  "errorPages":{
    "404":"src/test/resources/basic/assets/404.html"
  }
}